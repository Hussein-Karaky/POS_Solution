    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/flip.css" />
    <link rel="stylesheet" href="~/css/banner.css" />
    <link rel="stylesheet" href="~/css/nav.css" />
    <link rel="stylesheet" href="~/css/glass.css" />
    <link rel="stylesheet" href="~/css/content.css" />
    <link rel="stylesheet" href="~/css/loading.css" />
    <link rel="stylesheet" href="~/css/geo.css" />
    <link rel="stylesheet" href="~/css/footer.css" />
    <link rel="stylesheet" href="~/css/social.css" />
    <link rel="stylesheet" href="~/css/sched.css" />
    @*<link rel="stylesheet" href="~/css/fontawesome/v5.8.2/all.css" />*@
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" />
    <link rel="stylesheet" href="~/css/mdb/css/mdb.min.css" />
    <link rel="stylesheet" href="~/css/mdb/css/style.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons">
    <link rel="stylesheet" href="~/css/bootstrap-material-design.min.css" crossorigin="anonymous">
    @*<link rel="stylesheet" href="~/css/bootstrap-material-design.min.css" />*@
    @*<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">*@
    @RenderSection("Styles", required: false)
</head>
<body>
    <header>
        <!--Navbar-->
        <nav class="navbar navbar-expand-lg navbar-dark primary-color logged-out fixed-top">

            <!-- Navbar brand -->
            <a class="navbar-brand brand-link" asp-area="" asp-controller="Home" asp-action="Index">
                <i class="fas fa-book-reader"></i>
            </a>

            <!-- Collapse button -->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#basicExampleNav"
                    aria-controls="basicExampleNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <!-- Collapsible content -->
            <div class="collapse navbar-collapse" id="basicExampleNav">

                <!-- Links -->
                <ul class="navbar-nav mr-auto">
                    @RenderSection("Menus", required: false)
                    <!-- Dropdown -->
                    @*<li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown"
                               aria-haspopup="true" aria-expanded="false">Find A Tutor</a>
                            <div class="dropdown-menu dropdown-primary" aria-labelledby="navbarDropdownMenuLink">
                                <a class="dropdown-item" href="#">Search For Tutors</a>
                                <a class="dropdown-item" href="#">Request A Tutor</a>
                                <a class="dropdown-item" href="#">Online Tutoring</a>
                            </div>
                        </li>*@

                </ul>
                <!-- Links -->

                <form class="form-inline sign-btn-group">
                    @*<div class="md-form my-0">
                            <input class="form-control mr-sm-2" type="text" placeholder="Search" aria-label="Search">
                        </div>*@
                    <a class="navbar-brand" asp-area="" asp-controller="" asp-action="" data-toggle="modal" data-target="#fluidModalLeftInfo">
                        <i class="far fa-question-circle"></i>
                    </a>
                    <a id="navbar-static-signin" alt="Sign In" class="white-text align-self-center ml-2 auth-modal-toggle" data-auth-modal-tab="sign-in" data-toggle="modal" data-target="#modalLRForm">Log In</a>&nbsp;&nbsp;
                    <a id="navbar-static-signup" alt="Sign Up" class="btn btn-info btn-rounded light-blue lighten-5 blue-text btn-sm waves-effect waves-light auth-modal-toggle" data-auth-modal-tab="sign-up" data-toggle="modal" data-target="#modalLRForm">SIGN UP</a>
                </form>
            </div>
            <!-- Collapsible content -->

        </nav>
        <!--/.Navbar-->
    </header>
    <div class="container-fluid">
        <div name="top" class="clear"></div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <!--Modal: Login / Register Form-->
        @await Html.PartialAsync("../Shared/LoginView", new LoginModel())
        <!--Modal: Login / Register Form-->
        <!--Modal: Help-->
        <div class="modal fade right" id="fluidModalLeftInfo" tabindex="-1" role="dialog" data-backdrop="true" aria-modal="true">
            <div class="modal-dialog modal-full-height modal-right modal-notify modal-info" role="document">
                <!--Content-->
                <div class="modal-content">
                    <!--Header-->
                    <div class="modal-header light-blue darken-1">
                        <p class="heading lead">Help &amp; Support</p>

                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true" class="white-text">×</span>
                        </button>
                    </div>

                    <!--Body-->
                    <div class="modal-body">


                    </div>

                    <!--Footer-->
                    <div class="modal-footer justify-content-center">
                        <a class="btn btn-info waves-effect waves-light light-blue darken-1">
                            Great! Thanks
                            <i class="far fa-thumbs-up"></i>
                        </a>
                        <a class="btn btn-outline waves-effect" data-dismiss="modal">
                            Didn't help
                            <i class="far fa-frown"></i>
                        </a>
                    </div>
                </div>
                <!--/.Content-->
            </div>
        </div>
        <!--Modal: Help-->

        <div class="text-center light-blue darken-1" style="display:none;">
            <a href="javascript:window.scrollTo({top: 0, behavior: 'smooth'});" class="btn btn-default btn-rounded my-3">
                Back to top
            </a>
        </div>
    </div>
    <div id="signals">
        <span class="signal s1"></span>
        <span class="signal s2"></span>
        <span class="signal s3"></span>
        <span class="signal s4"></span>
    </div>
    <a href="javascript:window.scrollTo({top: 0, behavior: 'smooth'});" id="toTopBtn" class="btn btn-info btn-rounded btn-sm waves-effect waves-light cd-top text-replace js-cd-top cd-top--is-visible cd-top--fade-out light-blue darken-1" data-abc="true"></a>
    <!-- Footer -->
    <div class="container-fluid justify-content-center">
        <hr class="mx-0 px-0">
        <footer>
            <div class="row justify-content-around mb-0 pt-5 pb-0 ">
                <div class=" col-11">
                    <div class="row justify-content-center">
                        <div class="col-md-3 col-12 font-italic align-items-center mt-md-3 mt-4">
                            <h5><span><i class="fas fa-book-reader"></i></span><br /><b class="text-dark" style="margin-left: 4px;"> IN<span class="text-muted">TO</span></b></h5>
                            <p class="social mt-md-3 mt-2">
                                <a class="fa-lg p-2 m-2 fb-ic" style="color: #3b5998 !important;">
                                    <i class="fab fa-facebook-f"></i>
                                </a>
                                <a class="fa-lg p-2 m-2 tw-ic" style="color: #55acee !important;">
                                    <i class="fab fa-twitter"></i>
                                </a>
                                <a class="fa-lg p-2 m-2 li-ic" style="color: #0082ca !important;">
                                    <i class="fab fa-linkedin-in"></i>
                                </a>
                                <a class="fa-lg p-2 m-2 ins-ic" style="color: #2e5e86 !important;">
                                    <i class="fab fa-instagram"></i>
                                </a>
                                <a class="fa-lg p-2 m-2 yt-ic" style="color: #ed302f !important;">
                                    <i class="fab fa-youtube"></i>
                                </a>
                                <a class="fa-lg p-2 m-2 gplus-ic" style="color: #dd4b39 !important;">
                                    <i class="fab fa-google-plus-g"></i>
                                </a>
                            </p>
                            <small class="copy-rights cursor-pointer">&#9400; 2019 GEDA Technologies</small><br>
                            <small>Copyright.All Rights Resered. </small>

                            <div class="input-group mb-3">
                                <input type="text" class="form-control form-control-sm" placeholder="Your Email" aria-label="Your Email" aria-describedby="basic-addon2">
                                <div class="input-group-append">
                                    <button class="btn btn-info btn-rounded btn-sm light-blue darken-1" type="button" style="height: 62%;">
                                        <b>
                                            <svg viewBox="-5 16 128 128" class="mail" style="width: 18px; height: 22px;">
                                                <g stroke="white" stroke-width="4" fill="none">
                                                    <rect x="0" y="10" width="120" height="100" rx="15" ry="15" />
                                                </g>
                                                <g fill="none" stroke="white" stroke-width="4" stroke-linecap="round" stroke-linejoin="round">
                                                    <path d="M3.2 16 C -3 20, 10 42, 16 46 L60 80"></path>
                                                    <path transform="translate(120,0) scale(-1,1)" d="M3.2 16 C -3 20, 10 42, 16 46 L60 80"></path>
                                                </g>
                                            </svg>
                                        </b>
                                    </button>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3 col-12 my-sm-0 mt-5">
                            <ul class="list-unstyled">
                                <li class="mt-md-3 mt-4"><strong>Our Solution</strong></li>
                                <li>Intergrated Security Platform</li>
                                <li>Core Features</li>
                                <li>Product Features</li>
                                <li>Pricing</li>
                            </ul>
                        </div>
                        <div class="col-md-3 col-12 my-sm-0 mt-5">
                            <ul class="list-unstyled">
                                <li class="mt-md-3 mt-4"><strong>Resources</strong></li>
                                <li>Support</li>
                                <li>Blog</li>
                                <li>Product Features</li>
                                <li>Pricing</li>
                            </ul>
                        </div>
                        <div class="col-xl-auto col-md-3 col-12 my-sm-0 mt-5">
                            <ul class="list-unstyled">
                                <li class="mt-md-3 mt-4"><strong>Company</strong></li>
                                <li>About</li>
                                <li>Contact Us</li>
                                <li>Jobs</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <!-- Footer -->
