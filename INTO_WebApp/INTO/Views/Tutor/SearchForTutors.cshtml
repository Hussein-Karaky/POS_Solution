                <div class="card-body show pt-0" data-name="country">
                    <h4 class="heading mb-4 pb-1">Select Country</h4>

                    <div class="demo-wrapper" style="width: 800px; max-height: 400px;">
                        <div class="demo-container">
                            <div id="svgMapPopulation">

                            </div>
                        </div>
                    </div>
                    <div class="form-group" style="margin-top:1em;">
                        <select id="txtCountry" name="txtCountry" class="country-select" data-role="select-country" required>
                            <option value="">Select your country</option>
                            @foreach (var country in countries)
                            {
                                <option value="@country.Id" data-code="@country.Alpha2Code">@country.Name</option>
                            }
                        </select>
                    </div>
                    <div class="row justify-content-center"><button class="btn btn-outline next mx-2" id="next0" onclick="validate1(0)">Next</button> </div>
                </div>
                <div class="card-body pt-0" data-name="service">
                    <h4 class="heading mb-4 pb-1">@searchStep1[0]</h4>
                    <div class="row radio-group justify-content-between px-3">
                        <div class="col-md-12 margin-bottom">
                            <h5>@searchStep1[3]</h5>
                        </div>
                    </div>
                    @foreach (var service in TutorServices.Where<LookUpDetails>(l => l.Context.Equals("Online")))
                    {
                        <div class="row radio-group justify-content-between px-3">
                            <div class="col-md-12 margin-bottom tutor-margin-left">
                                <div>
                                    <div class="tutor-radio">
                                        <input type="radio" name="tutorServiceRadio" id="rbOnline_@service.Keyword" class="choice-tutoring-service option-input btn-outline square-24" value="@service.LookupDetailsId" data-role="tutoring-service">
                                        <label for="rbOnline_@service.Keyword" class="tutor-radio-label">
                                            <span class="label-text">@service.LookupDetailsDescription</span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                    <div class="row radio-group justify-content-between px-3">
                        <div class="col-md-12 margin-bottom">
                            <h5>@searchStep1[1]</h5>
                        </div>
                    </div>
                    @foreach (var service in TutorServices.Where<LookUpDetails>(l => l.Context.Equals("InPerson")))
                    {
                        <div class="row radio-group justify-content-between px-3">
                            <div class="col-md-12 margin-bottom tutor-margin-left">
                                <div class="tutor-radio">
                                    <input type="radio" name="tutorServiceRadio" id="rbFaceToFace_@service.Keyword" class="choice-tutoring-service option-input btn-outline square-24" value="@service.LookupDetailsId" data-role="tutoring-service">
                                    <label for="rbFaceToFace_@service.Keyword" class="tutor-radio-label">
                                        <span class="label-text"> @service.LookupDetailsDescription</span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    }
                    <div class="row radio-group justify-content-between px-3">
                        <div class="col-md-12 margin-bottom">
                            <h5>@searchStep1[2]</h5>
                        </div>
                    </div>
                    @foreach (var service in TutorServices.Where<LookUpDetails>(l => l.Context.Equals("School")))
                    {
                        <div class="row radio-group justify-content-between px-3">
                            <div class="col-md-12 margin-bottom tutor-margin-left">
                                <div class="tutor-radio">
                                    <input type="radio" name="tutorServiceRadio" id="rbTutorForSchool_@service.Keyword" class="choice-tutoring-service option-input btn-outline square-24" value="@service.LookupDetailsId" data-role="tutoring-service">
                                    <label for="rbTutorForSchool_@service.Keyword" class="tutor-radio-label">
                                        <span class="label-text"> @service.LookupDetailsDescription</span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    }
                    <div class="row radio-group justify-content-between px-3">
                        <div class="col-md-12 margin-bottom">
                            <div class="tutor-radio">
                                <input type="radio" name="tutorServiceRadio" id="rbTutorForSchool" class="choice-tutoring-service option-input btn-outline square-24" value="0" data-role="tutoring-service">
                                <label for="rbTutorForSchool" class="tutor-radio-label">
                                    <span class="label-text"> Not sure yet</span>
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="row justify-content-center"><button class="btn btn-outline prev mx-2">Back</button> <button class="btn btn-outline next mx-2" id="next1" onclick="validate1(0)">Next</button></div>
                </div>
                <div class="card-body pt-0" data-name="academic-level">
                    <h4 class="heading mb-4 pb-1">@searchStep3[0]</h4>
                    @foreach (var instituteType in InstituteTypes)
                    {
                        <div class="row radio-group justify-content-between px-3">
                            <div class="col-md-12 margin-bottom">
                                <div class="tutor-radio">
                                    <label for="rbLevel_@instituteType.LookupDetailsId" class="tutor-radio-label">
                                        <input type="radio" value="@instituteType.LookupDetailsId" id="rbLevel_@instituteType.LookupDetailsId" name="userLevelRadio" class="choice-user-academic-level option-input btn-outline square-24">
                                        <span class="label-text">@instituteType.LookupDetailsDescription</span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    }
                    <div class="row radio-group justify-content-between px-3">
                        <div class="col-md-12 margin-bottom">
                            <div class="tutor-radio">
                                <input type="radio" name="userLevelRadio" id="rbTutorForSchool" class="choice-tutoring-service option-input btn-outline square-24" value="0" data-role="tutoring-service">
                                <label for="rbTutorForSchool" class="tutor-radio-label">
                                    <span class="label-text"> Not sure yet</span>
                                </label>
                            </div>
                        </div>
                    </div>

                    <div class="row justify-content-center"><button class="btn btn-outline prev mx-2">Back</button> <button class="btn btn-outline next mx-2" id="next2">Next</button> </div>
                </div>
                <div class="card-body pt-0" data-name="curriculum">
                    <h4 class="heading mb-4 pb-1">@searchStep4[0]</h4>
                    <div class="form-group">
                        <select id="txtCurriculum" name="txtCurriculum" class="curriculum-select" data-role="choice-curriculum">
                            <option value="">Select your curriculum</option>
                            @foreach (var curriculum in Curriculum)
                            {

                                <option value="@curriculum.LookupDetailsId">@curriculum.LookupDetailsDescription</option>
                            }
                        </select>
                    </div>
                    <div class="row justify-content-center"><button class="btn btn-outline prev mx-2">Back</button> <button class="btn btn-outline next mx-2" id="next3">Next</button> </div>
                </div>
                <div class="card-body pt-0" data-name="education-class">
