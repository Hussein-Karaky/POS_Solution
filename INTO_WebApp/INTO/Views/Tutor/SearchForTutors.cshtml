                                    @foreach (var educationClass in Classes)
                                    {
                                        <option value="@educationClass.LookupDetailsId">@educationClass.LookupDetailsDescription</option>
                                    }
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row justify-content-center"><button class="btn btn-outline prev mx-2">Back</button> <button class="btn btn-outline next mx-2" id="next4">Next</button> </div>
